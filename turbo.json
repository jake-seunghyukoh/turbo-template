{
  "globalEnv": ["NEXT_PUBLIC_LINKEDIN_URL", "NEXT_PUBLIC_GITHUB_URL", "NEXT_PUBLIC_EMAIL_URL", "NEXT_PUBLIC_CHANNELTALK_SCRIPT", "KAKAO_CLIENT_ID", "KAKAO_CLIENT_SECRET"],
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
